<?xml version="1.0"?>
<definition>
  <source>

	<schema>anmn_ts</schema>
	<dataTable>select ts_id as instance_id, * from measurement</dataTable>
	<instanceTable>select * from timeseries</instanceTable>
  </source>


  <dimensions>
    <dimension name="TIME"/>
  </dimensions>
  <variables>
    <variable>
      <name>LATITUDE</name>
      <encoder>float</encoder>
      <attributes>
        <attribute name="_FillValue" value="99999."/>
      </attributes>
    </variable>
    <variable>
      <name>LONGITUDE</name>
      <encoder>float</encoder>
      <attributes>
        <attribute name="_FillValue" value="99999."/>
      </attributes>
    </variable>

    <variable>
      <name>TEMP</name>
      <encoder>float</encoder>
      <dimensions>
        <dimension name="TIME"/>
      </dimensions>
      <attributes>
        <attribute name="_FillValue" value="99999."/>
        <attribute name="whoot" value="whootvalue"/>
      </attributes>
    </variable>
    <variable>
      <name>DEPTH</name>
      <encoder>float</encoder>
      <dimensions>
        <dimension name="TIME"/>
      </dimensions>
      <attributes>
        <attribute name="_FillValue" value="99999."/>
      </attributes>
    </variable>
    <variable>
      <name>DEPTH_quality_control</name>
      <encoder>byte</encoder>
      <dimensions>
        <dimension name="TIME"/>
      </dimensions>
      <attributes>
        <attribute name="_FillValue" value="0xffb"/>
      </attributes>
    </variable>

  </variables>
</definition>
