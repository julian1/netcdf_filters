<?xml version="1.0"?>
<definition>

  <source>
	<schema>anmn_nrs_ctd_profiles</schema>
	<dataTable>select file_id as instance_id, * from measurements</dataTable>
	<instanceTable>select * from indexed_file</instanceTable>
  </source>


  <dimensions>
    <dimension name="DEPTH"/>
  </dimensions>
  <variables>
    <variable>
      <name>LATITUDE</name>
      <encoder>float</encoder>
      <attributes>
        <attribute name="_FillValue" value="99999."/>
      </attributes>
    </variable>
    <variable>
      <name>TEMP</name>
      <encoder>float</encoder>
      <dimensions>
        <dimension name="DEPTH"/>
      </dimensions>
      <attributes>
        <attribute name="_FillValue" value="99999."/>
        <attribute name="whoot" value="whootvalue"/>
      </attributes>
    </variable>
    <variable>
      <name>DEPTH_quality_control</name>
      <encoder>byte</encoder>
      <dimensions>
        <dimension name="DEPTH"/>
      </dimensions>
      <attributes>
        <attribute name="_FillValue" value="0xffb"/>
      </attributes>
    </variable>
    <variable>
      <name>DEPTH</name>
      <encoder>float</encoder>
      <dimensions>
        <dimension name="DEPTH"/>
      </dimensions>
      <attributes>
        <attribute name="_FillValue" value="99999."/>
        <attribute name="units" value="days since 1950-01-01 00:00:00 UTC"/>
      </attributes>
    </variable>
  </variables>
</definition>
